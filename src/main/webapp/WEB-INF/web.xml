<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>ua.edu.npu.HelloServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/servlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>JsonServlet</servlet-name>
    <servlet-class>ua.edu.npu.FromJsonServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>JsonServlet</servlet-name>
    <url-pattern>/json</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>JsonToServlet</servlet-name>
    <servlet-class>ua.edu.npu.ToJsonServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>JsonToServlet</servlet-name>
    <url-pattern>/jsongetdata</url-pattern>
  </servlet-mapping>
</web-app>
